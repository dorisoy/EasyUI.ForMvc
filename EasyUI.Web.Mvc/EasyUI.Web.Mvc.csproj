  <Target Name="AfterBuildDebug">
    <MergeFiles Targets="@(EditorJavaScriptFiles)" Output="Scripts\EasyUI.editor.js" />
    <Replace Files="Scripts\EasyUI.editor.js" Pattern="DearReplaceCommandPleaseRemoveTheBomCharacter" Replacement="" Multiline="false" />
    <MergeFiles Targets="@(ChartScripts)" Output="Scripts\EasyUI.chart.js" />
    <Replace Files="Scripts\EasyUI.chart.js" Pattern="DearReplaceCommandPleaseRemoveTheBomCharacter" Replacement="" Multiline="false" />
    <!-- Copy the CSS files to Scripts -->
    <Copy SourceFiles="@(Content)" DestinationFolder="$(OutputPath)\Scripts" Condition="'%(Extension)'=='.js'">
      <Output TaskParameter="DestinationFiles" ItemName="Resources" />
    </Copy>
    <!-- Copy the CSS and Image files to Content -->
    <ItemGroup>
      <CssAndImageFiles Include="Content\**\*.*">
        <Visible>False</Visible>
      </CssAndImageFiles>
    </ItemGroup>
    <Copy SourceFiles="@(CssAndImageFiles)" DestinationFolder="$(OutputPath)\Content\%(RecursiveDir)">
      <Output TaskParameter="DestinationFiles" ItemName="Resources" />
    </Copy>
    <ItemGroup>
      <!-- FileWrites is the list of files to delete when performing a Clean -->
      <FileWrites Include="@(Resources)" />
    </ItemGroup>
  </Target>
  <Target Name="AfterBuildRelease">
    <!-- Get the assembly version -->
    <GetVersion AssemblyFiles="$(IntermediateOutputPath)$(AssemblyName).dll">
      <Output TaskParameter="Assemblies" ItemName="Info" />
    </GetVersion>
    <!-- Store it in property -->
    <PropertyGroup>
      <Version>%(Info.Version)</Version>
    </PropertyGroup>
    <MergeFiles Targets="@(EditorJavaScriptFiles)" Output="Scripts\EasyUI.editor.js" />
    <Replace Files="Scripts\EasyUI.editor.js" Pattern="DearReplaceCommandPleaseRemoveTheBomCharacter" Replacement="" Multiline="false" Encoding="Windows-1252" />
    <MergeFiles Targets="@(ChartScripts)" Output="Scripts\EasyUI.chart.js" />
    <Replace Files="Scripts\EasyUI.chart.js" Pattern="DearReplaceCommandPleaseRemoveTheBomCharacter" Replacement="" Multiline="false" />
    <!-- Copy all JavaScript to $(OutputPath)\Scripts, add versioning and ".min" suffix -->
    <ItemGroup>
      <JavaScriptFilesToSkip Include="Scripts\*.min.js" />
      <JavaScriptFilesToMinify Include="Scripts\*.js" Exclude="@(JavaScriptFilesToSkip)" />
    </ItemGroup>
    <Message Importance="High" Text="Copy @(JavaScriptFilesToMinify)" />
    <Copy SourceFiles="@(JavaScriptFilesToMinify)" DestinationFiles="@(JavaScriptFilesToMinify -> '$(OutputPath)\Scripts\$(Version)\%(Filename).min.js')">
      <Output TaskParameter="DestinationFiles" ItemName="JavaScriptFiles" />
    </Copy>
    <Message Importance="High" Text="Copied @(JavaScriptFiles)" />
    <!-- Minify the JavaScript files -->
    <CompressorTask JavaScriptFiles="%(JavaScriptFiles.Identity)" JavaScriptOutputFile="%(JavaScriptFiles.Identity)" IsEvalIgnored="true" LineBreakPosition="-1" PreserveAllSemicolons="false" DisableOptimizations="false" ObfuscateJavaScript="true" LoggingType="ALittleBit" />
    <Copy SourceFiles="@(JavaScriptFilesToSkip)" DestinationFolder="$(OutputPath)\Scripts\$(Version)">
      <Output TaskParameter="DestinationFiles" ItemName="JavaScriptFiles" />
    </Copy>
    <!-- Copy all CSS to $(OutputPath)\Content, add versioning and ".min" suffix -->
    <ItemGroup>
      <CssFilesToSkip Include="Content\**\*.custom.css" />
      <CssFilesToMinify Include="Content\**\*.css" Exclude="@(CssFilesToSkip)" />
    </ItemGroup>
    <Copy SourceFiles="@(CssFilesToMinify)" DestinationFiles="@(CssFilesToMinify -> '$(OutputPath)\Content\$(Version)\%(Filename).min.css')">
      <Output TaskParameter="DestinationFiles" ItemName="CssFiles" />
    </Copy>
    <!-- Minify the CSS files -->
    <CompressorTask CssFiles="%(CssFiles.Identity)" CssOutputFile="%(CssFiles.Identity)" LineBreakPosition="0" PreserveAllSemicolons="true" DisableOptimizations="false" LoggingType="ALittleBit" />
    <Copy SourceFiles="@(CssFilesToSkip)" DestinationFolder="$(OutputPath)\Content\$(Version)\%(RecursiveDir)">
      <Output TaskParameter="DestinationFiles" ItemName="CssFiles" />
    </Copy>
    <!-- Copy the images and apply versioning -->
    <ItemGroup>
      <ImageFiles Include="Content\**\*.*">
        <Visible>False</Visible>
      </ImageFiles>
    </ItemGroup>
    <Copy SourceFiles="@(ImageFiles)" DestinationFolder="$(OutputPath)\Content\$(Version)\%(RecursiveDir)" Condition="'%(Extension)'!='.css'">
      <Output TaskParameter="DestinationFiles" ItemName="Resources" />
    </Copy>
    <ItemGroup>
      <FileWrites Include="@(Resources)" />
      <FileWrites Include="@(JavaScriptFiles)" />
      <FileWrites Include="@(CssFiles)" />
    </ItemGroup>
  </Target>
